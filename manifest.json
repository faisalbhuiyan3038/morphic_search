{
  "manifest_version": 2,
  "name": "Morphic Search Engine",
  "description": "Adds morphic.sh as search engine.",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Morphic",
      "search_url": "https://www.morphic.sh/search?q={searchTerms}",
      "keyword": "!m, @morphic",
      "favicon_url": "https://www.morphic.sh/icon.png",
      "is_default": true,
      "encoding": "UTF-8"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "68.0"
    }
  }
}
